<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

<form action="/upload" method="post" enctype="multipart/form-data">
  <input type="text" name="name">
  <input type="file" name="picture">
  <input type="submit" value="upload">
</form>

{{#each pictures}}
  <h1>{{_id}}</h1>
  <img src="{{path}}" alt="{{name}}">
{{/each}}